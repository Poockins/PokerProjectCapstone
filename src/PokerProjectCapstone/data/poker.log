/*C3*/SET SCHEMA PUBLIC
CREATE TABLE IF NOT EXISTS games (id INT IDENTITY PRIMARY KEY, flop VARCHAR(30) ARRAY, turn VARCHAR(30), river VARCHAR(30), created_at TIMESTAMP)
CREATE TABLE IF NOT EXISTS players (id INT IDENTITY PRIMARY KEY, name VARCHAR(256))
CREATE TABLE IF NOT EXISTS hands (id INT IDENTITY PRIMARY KEY, cards VARCHAR(30) ARRAY, player_id INT, game_id INT, FOREIGN KEY(player_id) REFERENCES players, FOREIGN KEY(game_id) REFERENCES games)
